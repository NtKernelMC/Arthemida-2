msgid "Install DirectX"
msgstr "Установите DirectX"

#. INST_SEC_CLIENT
#: ../Shared/installer/nightly.nsi:462
msgid "Game client"
msgstr "Игровой клиент"

#. INST_SEC_SERVER
#: ../Shared/installer/nightly.nsi:463
msgid "Dedicated server"
msgstr "Выделенный сервер"

